{
  "name": "xydrag",
  "version": "0.0.1",
  "description": "Fast and lightweight draggable library with a plugin system to extend it",
  "author": "Maciej Krawczyk",
  "homepage": "https://github.com/mckravchyk/xydrag",
  "license": "MIT",
  "private": true,
  "main": "./dist/xydrag.common.js",
  "module": "./dist/xydrag.esm.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "lint": "eslint --ext ts src",
    "check-types": "tsc --noemit",
    "test": "npx jest",
    "check": "npm run lint; npm run check-types && npm run test",
    "dev": "dev=true rollup -c -w",
    "build": "rollup -c",
    "prepublish": "npm run check && npm run build"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "draggable",
    "drag",
    "move"
  ],
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "babel-jest": "^29.6.1",
    "eslint": "^8.45.0",
    "eslint-config-mckravchyk": "^1.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^3.26.3",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-typescript2": "^0.35.0",
    "tslib": "^2.4.0",
    "typescript": "5.2"
  },
  "dependencies": {
    "event-listener-extended": "^1.0.0"
  }
}
